{
    "name": "netzkultur/gearmanmanager",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Sven Mulholland",
            "email": "mulholland@netzkultur.de"
        }
    ],
    "repositories": [
        {
            "type": "git",
